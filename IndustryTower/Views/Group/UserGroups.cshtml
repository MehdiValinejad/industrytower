@model IEnumerable<IndustryTower.Models.Group>
@if (Model.Count() > 0)
{
    <div class="user-groups list-group dir no-pad">
        @foreach (var item in Model)
        {
             @Html.ActionLink(item.groupName, "GroupPage", new { GId = item.groupId, GName = StringHelper.URLName(item.groupName) }, new { @class = "ugroup list-group-item" })
        }
    </div>
}
else
{
    <div class="text-center">
        @Resource.noResult
    </div>
}
